body
  background #fafafa

.chart
  margin 40px auto
  display block

  text
    font-size 10px
    font-family 'Helvetica'
    color #666
    font-weight 100

  circle
    fill #135DB8

    &.red
      fill #135DB8

    &.blue
      fill #CD0016

  .axis
    path,
    line
      fill none
      stroke #999
      shape-rendering crispEdges
